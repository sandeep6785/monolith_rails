<meta charset="utf-8">
<title>MonolithHomepage</title>
<meta content="text/html; charset=UTF-8" http-equiv="Content-Type" />
<meta content="width=device-width, initial-scale=1.0, maximum-scale=1" name="viewport">
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
<%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
<%= stylesheet_link_tag 'main', media: 'screen' %>
<%= csrf_meta_tags %>

<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
<script src="https://code.jquery.com/ui/1.11.4/jquery-ui.js" type="text/javascript"></script>
<%= javascript_include_tag 'main' %>
<%= javascript_include_tag 'plugins/jquery.fitvids' %>
<%= javascript_include_tag 'plugins/jquery.stellar.min' %>
<%= javascript_include_tag 'plugins/isotope.pkgd.min' %>
<%= javascript_include_tag 'plugins/jquery.appear' %>
<%= javascript_include_tag 'plugins/jquery.fs.tipper.min' %>
<%= javascript_include_tag 'plugins/jquery.countTo' %>
<%= javascript_include_tag 'plugins/owl.carousel.min' %>
<%= javascript_include_tag 'plugins/jquery.easing.1.3' %>
<!-- SLIDER REVOLUTION SCRIPTS  -->
<%= javascript_include_tag 'rs-plugin/js/jquery.themepunch.tools.min' %>
<%= javascript_include_tag 'rs-plugin/js/jquery.themepunch.revolution.min' %>
<!-- End SLIDER REVOLUTION SCRIPTS  -->
<%= javascript_include_tag 'plugins/jquery.magnific-popup.min' %>
<%= javascript_include_tag 'plugins/jquery.downCount' %>
<%= javascript_include_tag 'plugins/jquery.singlePageNav.min' %>
<%= javascript_include_tag 'plugins/wow.min' %>
<%= javascript_include_tag 'plugins/jquery.validate.min' %>
<%= javascript_include_tag 'contact-form' %>
<script src="https://maps.googleapis.com/maps/api/js?sensor=false" type="text/javascript"></script>
<%= javascript_include_tag 'map' %>
<%= javascript_include_tag 'theme' %>

<!--[if lt IE 9]>
  <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.6/html5shiv.min.js"></script>
<![endif]-->
<%= yield :head %>
